int main() {
    // Переменные
    int a = 555, b = 59, c = 1004, d = 8;
    float e = 0.100000, f = 0.200000, sum = 0.3000000000000004;

    // Вывод
    printf("|%d|\n|%7d|\n|%-7d|\n", a, a, a);
    printf("Words:       %d\n", b);
    printf("Letters:   %d\n", c);
    printf("Digits:       %d\n", d);
    printf("a = %f; b = %f\na + b = %f\n", e, f, sum);

    return 0; // Завершение программы
}
